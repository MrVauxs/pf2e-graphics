		"@typhonjs-fvtt/svelte-standard": "^0.1.0"
	},
	"devDependencies": {
		"@actions/core": "^1.10.1",
		"@antfu/eslint-config": "^2.27.2",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tsconfig/svelte": "^5.0.4",
		"@types/lodash": "^4.17.6",
