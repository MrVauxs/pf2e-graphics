		"svelte-jsoneditor": "^0.23.8",
		"svelte-preprocess": "^6.0.1",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.7.0",
		"typescript": "^5.4.5",
		"vite": "^5.4.2",
		"vite-plugin-checker": "^0.7.2",
		"vite-tsconfig-paths": "^5.0.1"
	}
